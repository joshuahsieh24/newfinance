{
  "name": "financeai",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently -k -n WEB,ML \"pnpm --filter web dev\" \"uvicorn services.ml-api.main:app --reload --port 8000\"",
    "lint": "pnpm --filter web lint",
    "start": "pnpm --filter web start && pnpm --filter ml-api start"
  },
  "devDependencies": {
    "concurrently": "^8"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^3.3.1"
  }
}
